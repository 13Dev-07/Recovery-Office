    "type-check:theme": "node ts-check.js src/design-system/theme/theme.ts",
    "type-check:all": "node ts-check.js src/**/*.{ts,tsx}"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
