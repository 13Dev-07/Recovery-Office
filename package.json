{
  "name": "recovery-office",
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "generate:sitemap": "ts-node src/scripts/generateSitemap.ts",
    "build": "npm run generate:sitemap && react-scripts build",
    "validate:typescript": "ts-node src/scripts/validateTypeScript.ts",
    "validate:sacred-geometry": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node src/scripts/validateSacredGeometry.ts",
    "validate:responsive": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node src/scripts/validateResponsive.ts",
    "validate:a11y": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node src/scripts/validateAccessibility.ts",
    "validate:performance": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node src/scripts/validatePerformance.ts",
    "validate:cross-browser": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node src/scripts/validateCrossBrowser.ts",
    "validate:security": "cross-env NODE_OPTIONS=\"--loader ts-node/esm\" ts-node src/scripts/validateSecurity.ts",
    "validate:all": "npm run validate:sacred-geometry && npm run validate:responsive && npm run validate:performance && npm run validate:cross-browser && npm run validate:security && npm run validate:types && npm run validate:lint && npm run validate:a11y",
    "prebuild": "npm run validate:all",
    "deploy:now": "cross-env SKIP_TYPESCRIPT_CHECK=true TSC_COMPILE_ON_ERROR=true react-scripts build",
    "build:quick": "tsc --skipLibCheck --noEmit false && node build.js",
    "deploy": "cross-env SKIP_TS_CHECK=true node scripts/deploy.js"
  },
  "dependencies": {
    "@axe-core/puppeteer": "^4.7.3",
    "@types/styled-components": "^5.1.34",
    "chrome-launcher": "^1.1.0",
    "framer-motion": "^12.9.2",
    "lighthouse": "^11.1.0",
    "playwright": "^1.39.0",
    "puppeteer": "^22.8.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-router-dom": "^7.5.3",
    "styled-components": "^6.1.17",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@types/node": "^20.11.5",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@typescript-eslint/eslint-plugin": "^6.19.0",
    "@typescript-eslint/parser": "^6.19.0",
    "cross-env": "^7.0.3",
    "eslint": "^8.56.0",
    "eslint-config-prettier": "^9.1.0",
    "prettier": "^3.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.3.3"
  }
}
